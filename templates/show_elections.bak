{% extends "layout.html" %}
{% block body %}
  

  <div class="input-group">
    <span class="input-group-addon">Address</span>
    <input type="text" class="form-control" placeholder="123 main st, new york, ny, 10010">
    
  </div>
  <form action="{{ url_for('lookup_election') }}" method="post" class="add-entry">
      <dl>
        <dt>Address:
        <dd><input type="text" size="30" name="address">
        <dt>Election:
        <select name="election">
        {% for election, value in elections.items() %}
          <option value="{{election}}">{{value["name"]}}</option>    
        {% endfor %}
        </select>
        <dd><input type="submit" value="Lookup">
      </dl>
    </form>
{% endblock %}